class Solution {
public:
    int majorityElement(vector<int>& nums) {
        int cnt = 0;
        int c = nums[0];
        for(auto& a:nums) {
            if(a==c) cnt++;
            else if(cnt==0) c=a, cnt=1;
            else cnt--;
        }
        return c;
    }
};

